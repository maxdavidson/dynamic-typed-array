{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "outDir": "dist-es2015",
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": [
    "node_modules",
    "dist",
    "dist-es2015"
  ]
}
